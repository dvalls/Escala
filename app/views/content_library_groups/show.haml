- content_for :library_options do
  - @content_group.categories.each do |category|
    - case
      - when category.library_files.count > 0
        %span.glyphicon.glyphicon-menu-right{"aria-hidden" => "true"}
        = link_to category.name, content_library_group_category_path(@content_group, category.id)
        %br/
  = link_to 'voltar', library_files_path


= render partial: 'partials/categories_show'

