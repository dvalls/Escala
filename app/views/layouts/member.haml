
%html
  %head
    = render :partial => 'partials/head'

    %link{:crossorigin => "anonymous", :href => "https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css", :integrity => "sha384-BVYiiSIFeK1dGmJRAkycuHAHRg32OmUcww7on3RYdg4Va+PmSTsz/K68vbdEjh4u", :rel => "stylesheet"}/

  %body
    .container
      / Header
      %header
        = render partial: 'partials/member_navbar'
        = render 'partials/member_head'

        = render :partial => 'partials/flash_message'
      / Main content
      %main
        / Main
        .row
          - if @pages != nil
            = render partial: 'partials/member_page_options'
          = yield



      = stylesheet_link_tag "course"
      = javascript_include_tag "jquery.lazyload.min", "course"

      / Footer
      = render :partial => 'partials/footer'
